{
    "name": "svelte-ts",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test:unit": "vitest",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write ."
    },
    "devDependencies": {
        "@playwright/test": "1.40.*",
        "@skeletonlabs/skeleton": "2.5.*",
        "@skeletonlabs/tw-plugin": "0.2.*",
        "@sveltejs/adapter-auto": "2.1.*",
        "@sveltejs/adapter-static": "2.0.*",
        "@sveltejs/kit": "1.27.*",
        "@types/lodash": "4.14.*",
        "@typescript-eslint/eslint-plugin": "6.11.*",
        "@typescript-eslint/parser": "6.11.*",
        "autoprefixer": "10.4.*",
        "eslint": "8.54.*",
        "eslint-config-prettier": "9.0.*",
        "eslint-plugin-svelte": "2.35.*",
        "postcss": "8.4.*",
        "postcss-load-config": "4.0.*",
        "prettier": "3.1.*",
        "prettier-plugin-svelte": "3.1.*",
        "svelte": "4.2.*",
        "svelte-check": "3.6.*",
        "svelte-preprocess": "5.1.*",
        "tailwindcss": "3.3.*",
        "tslib": "2.6.*",
        "typescript": "5.2.*",
        "vite": "4.*",
        "vitest": "0.34.*"
    },
    "type": "module",
    "dependencies": {
        "lodash": "4.17.*",
        "mono-icons": "1.3.*"
    }
}
